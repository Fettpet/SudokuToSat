	@test -d .tmp/Sudoku1.0.0 || mkdir -p .tmp/Sudoku1.0.0
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/Sudoku1.0.0/ && (cd `dirname .tmp/Sudoku1.0.0` && $(TAR) Sudoku1.0.0.tar Sudoku1.0.0 && $(COMPRESS) Sudoku1.0.0.tar) && $(MOVE) `dirname .tmp/Sudoku1.0.0`/Sudoku1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/Sudoku1.0.0


